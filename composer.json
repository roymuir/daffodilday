{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.6.0",
		"silverstripe/framework": "3.6.0",
		"silverstripe/reports": "3.6.0",
		"silverstripe/siteconfig": "3.6.0",
		"silverstripe-themes/simple": "3.1.*",
		"unclecheese/betterbuttons": "^1.3",
		"silverstripe/userforms": "^4.3",
		"silverstripe-australia/gridfieldextensions": "^1.4",
		"gdmedia/ss-auto-git-ignore": "^1.0",
		"silverstripe/recaptcha": "^2.0",
		"swipestripe/swipestripe": "^2.1",
		"frankmullenger/payment-cheque": "dev-master",
		"swipestripe/swipestripe-flatfeeshipping": "dev-master",
		"silverstripe/payment": "dev-master",
		"swipestripe/swipestripe-addresses": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
